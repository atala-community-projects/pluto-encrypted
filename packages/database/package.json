{
  "name": "@pluto-encrypted/database",
  "version": "0.0.9",
  "description": "A \"secure\" DB Agnostic Wrapper for AtalaPrism -> Pluto storage",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "NODE_ENV=debug jest tests/*.test.ts",
    "build": "rm -rf build && npx rollup -c rollup/rollup.mjs",
    "coverage": "npm run test -- --coverage",
    "docs": "npx typedoc --options typedoc.json --hideGenerator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elribonazo/pluto-encrypted.git"
  },
  "author": "elribonazo@gmail.com",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@atala/prism-wallet-sdk": "^3.1.0"
  },
  "optionalDependencies": {
    "@pluto-encrypted/indexdb": "^0.0.8",
    "@pluto-encrypted/inmemory": "^0.0.8",
    "@pluto-encrypted/leveldb": "^0.0.7"
  },
  "dependencies": {
    "rxdb": "^14.17.0",
    "uuid": "^9.0.1"
  },
  "files": [
    "build/*"
  ],
  "devDependencies": {
    "@pluto-encrypted/indexdb": "^0.0.8",
    "@pluto-encrypted/inmemory": "^0.0.8",
    "@pluto-encrypted/leveldb": "^0.0.7"
  },
  "gitHead": "6aa4ce8bbb9938fc4c4e790efffa5b902631362c"
}